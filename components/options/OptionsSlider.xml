<?xml version="1.0" encoding="utf-8"?>
<component name="OptionsSlider" extends="PanelSet">
  <children>
    <ListPanel id="panel">
      <Rectangle id="backdrop"
        translation="[0, 0]"
        color="#00000000"
        width="582"
        height="1080" />
      <LabelList id="panelList" vertFocusAnimationStyle="floatingFocus">
        <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="34" />
        <Font role="focusedFont" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="34" />
        <ContentNode id="fieldList" role="content" />
      </LabelList>
    </ListPanel>
  </children>
  <interface>
    <field id="buttons" type="nodearray" onChange="setFields" />
    <field id="options" type="nodearray" onChange="setFields" />
    <field id="closeSidePanel" type="boolean" alwaysNotify="true" />
  </interface>
</component>