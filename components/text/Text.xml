<?xml version="1.0" encoding="utf-8"?>
<component name="Text" extends="Label">
    <interface>
        <field id="text" type="string" />
        <field id="wrap" type="boolean" />
        <field id="maxLines" type="integer" />
        <field id="width" type="float" />

        <field id="size" type="float" onChange="updateFont" alwaysNotify="true" />
        <field id="bold" type="boolean" onChange="updateFont" alwaysNotify="true" />
    </interface>
</component>