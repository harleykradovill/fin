<?xml version="1.0" encoding="utf-8"?>
<component name="JFServer" extends="Group">

  <interface>
    <field id="itemContent" type="node" onchange="itemContentChanged" />
    <field id="itemHasFocus" type="boolean" onchange="onItemHasFocusChange" />
  </interface>

  <children>
    <Rectangle id="background" translation="[0,0]" width="1620" height="100" />

    <Poster id="poster" translation="[5,5]" width="90" height="90" />
    <Group id="labels" translation="[120,0]">
      <Label text="Name:" horizAlign="left" font="font:SmallBoldSystemFont" width="130" height="65" translation="[0,15]" />
      <Label text="URL:" horizAlign="left" font="font:SmallBoldSystemFont" width="130" height="65" translation="[0,55]" />

      <Label id="name" horizAlign="left" font="font:SmallSystemFont" height="65" translation="[145,15]" />
      <Label id="baseUrl" horizAlign="left" font="font:SmallSystemFont" height="65" translation="[145,55]" />
    </Group>

    <Rectangle id="trashContainer" translation="[1480,0]" width="100" height="100">
      <Poster id="trash" width="50" height="50" uri="pkg:/images/icons/trash.png" translation="[25,25]" />
    </Rectangle>
  </children>

</component>