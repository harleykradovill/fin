<?xml version="1.0" encoding="utf-8"?>
<component name="JFServer" extends="Group">

  <interface>
    <field id="itemContent" type="node" onchange="itemContentChanged" />
    <field id="itemHasFocus" type="boolean" onchange="onItemHasFocusChange" />
  </interface>

  <children>
    <Rectangle id="background" translation="[0,0]" width="1620" height="100" />

    <Group id="labels" translation="[20,0]">
      <Label text="Name:" horizAlign="left" width="130" height="65" translation="[0,6]">
        <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="32" />
      </Label>
      <Label text="URL:" horizAlign="left" width="130" height="65" translation="[0,48]">
        <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="32" />
      </Label>

      <Label id="name" horizAlign="left" height="65" translation="[145,6]">
        <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="32" />
      </Label>
      <Label id="baseUrl" horizAlign="left" height="65" translation="[145,48]">
        <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="32" />
      </Label>
    </Group>

    <Rectangle id="trashContainer" translation="[1480,0]" width="100" height="100">
      <Poster id="trash" width="50" height="50" uri="pkg:/images/icons/trash.png" translation="[25,25]" />
    </Rectangle>
  </children>

</component>