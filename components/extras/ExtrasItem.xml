<?xml version="1.0" encoding="utf-8"?>
<component name="ExtrasItem" extends="JFGroup">
  <children>
    <LayoutGroup layoutDirection="vert">
      <MaskGroup maskUri="pkg:/images/icons/circle.png" maskSize="[156, 156]" maskBitmapWidth="256" maskBitmapHeight="256">
        <Poster id="posterImg" width="156" height="156" loadDisplayMode="scaleToZoom" failedBitmapUri="pkg:/images/baseline_person_white_48dp.png" />
      </MaskGroup>

      <ScrollingText
        id="pLabel"
        horizAlign="center"
        vertAlign="center"
        height="32"
        repeatCount="0"
        maxWidth="156"
        size="18" />

      <ScrollingText
        id="SubTitle"
        horizAlign="center"
        vertAlign="center"
        height="32"
        color="#777777FF"
        repeatCount="0"
        maxWidth="156"
        size="18" />

    </LayoutGroup>
  </children>
  <interface>
    <field id="itemContent" type="node" onChange="showContent" />
    <field id="itemHasFocus" type="boolean" onChange="focusChanged" />
  </interface>
</component>