<?xml version="1.0" encoding="utf-8"?>
<component name="Settings" extends="JFScreen">
  <children>

    <Label id="path" translation="[95, 175]" font="font:SmallestBoldSystemFont">
      <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="28" />
    </Label>
    <MultiStyleLabel id="version" translation="[95, 1000]" />

    <LabelList
      translation="[120, 250]"
      id="settingsMenu"
      font="font:SmallSystemFont"
      itemSize="[510, 54]"
      itemSpacing="[0,10]"
      numRows="11"
      vertFocusAnimationStyle="floatingFocus">
      <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="28" />
      <Font role="focusedFont" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="28" />
    </LabelList>

    <Poster translation="[710, 230]" id="testRectangle" width="1110" height="800" uri="pkg:/images/white.9.png" />

    <Label id="settingDesc" translation="[730, 270]" width="1065" wrap="true" horizAlign="left">
      <Font role="font" uri="pkg:/images/fonts/NotoSans-Regular.ttf" size="32" />
    </Label>

    <RadioButtonList id="radioSetting" numRows="6" vertFocusAnimationStyle="floatingFocus" translation="[1034, 510]" />

    <RadioButtonList id="boolSetting" vertFocusAnimationStyle="floatingFocus" translation="[1034, 510]">
      <ContentNode role="content">
        <ContentNode title="Disabled" />
        <ContentNode title="Enabled" />
      </ContentNode>
    </RadioButtonList>

    <IntegerKeyboard translation="[1034, 510]" id="integerSetting" domain="numeric" visible="false" />
    <Timer id="keypressTimer" duration="5" />
  </children>
</component>